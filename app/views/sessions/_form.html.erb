<%= form_for(:session, url:login_path) do |f| -%>

	<div class="field">
		<%= f.label :email, :class => "title" %> 
		<%= f.email_field :email %>
	</div>
	<div class="field">
		<%= f.label :password, :class => "title" %> 
		<%= f.password_field :password %>
	</div>
	<div class="field remember_me">
		<%= f.label :remember_me, :class => "title" %> 
		<%= f.check_box :remember_me %>
	</div>

	<div class="field submit">
		<%= f.submit "Log in"%>
	</div>
  
<% end -%>